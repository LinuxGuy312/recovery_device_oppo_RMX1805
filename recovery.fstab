/system_root      ext4     /dev/block/bootdevice/by-name/system           flags=display="System"
/system_image     emmc     /dev/block/bootdevice/by-name/system
/cache            ext4     /dev/block/bootdevice/by-name/cache
/data             ext4     /dev/block/bootdevice/by-name/userdata         flags=encryptable=footer
/vendor           ext4     /dev/block/bootdevice/by-name/vendor           flags=wipeingui;display="Vendor"
/external_sd      vfat     /dev/block/mmcblk1p1                           flags=removable;storage;wipeingui;display="Micro SDCard"
/usb_otg          vfat     /dev/block/sda1     /dev/block/sda             flags=removable;storage;wipeingui;display="USB OTG"
/boot             emmc     /dev/block/bootdevice/by-name/boot
/recovery         emmc     /dev/block/bootdevice/by-name/recovery         flags=backup=1
/misc             emmc     /dev/block/bootdevice/by-name/misc
/modem            emmc     /dev/block/bootdevice/by-name/modem            flags=backup=1;display="Modem"
/modemst1         emmc     /dev/block/bootdevice/by-name/modem_st1        flags=backup=1;subpartitionof=/modem
/modemst2         emmc     /dev/block/bootdevice/by-name/modem_st2        flags=backup=1;subpartitionof=/modem
/logo             emmc     /dev/block/bootdevice/by-name/LOGO
/dsp              emmc     /dev/block/bootdevice/by-name/dsp              flags=backup=1;subpartitionof=/modem
/persist          ext4     /dev/block/bootdevice/by-name/persist          flags=backup=1;display="Persist"
